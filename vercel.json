{
  "version": 2,
  "builds": [
    {
      "src": "manage.py",
      "use": "@vercel/python"
    },
    {
      "src": "Cesim/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "staticfiles_build/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles_build/$1"
    },
    {
      "src": "/(.*)",
      "dest": "manage.py"
    }
  ],
  "ignore": ["venv", "venv1", "__pycache__", "node_modules"],
  "env": {
    "DJANGO_SETTINGS_MODULE": "Cesim.settings"
  }
}